# Alai Challenge Environment Variables
# ----------------------------------
# This file contains environment variables required by the application.
# IMPORTANT: Never commit this file to version control. Add it to .gitignore.
# Make a copy of this file named .env and fill in your actual credentials.

# Firecrawl API Configuration
# --------------------------
# Sign up at https://www.firecrawl.dev/ to get your free API key
# This key is required for the web scraping functionality
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Alai Authentication
# -----------------
# Your Alai account credentials used for API authentication
# Create an account at https://www.getalai.com if you don't have one
ALAI_EMAIL=your_email@example.com
ALAI_PASSWORD=your_secure_password

ALAI_Create_Slide= "
https://alai-standalone-backend.getalai.com/create-new-slide" 
ALAI_SLIDE_VARIENT="https://alai-standalone-backend.getalai.com/create-slide-variant-from-element-slide"

ALAI_DELETE_Slide= "https://alai-standalone-backend.getalai.com/delete-slides"


ALAI_UPDATE_ELEMENTS= "https://alai-standalone-backend.getalai.com/update-slide-entity"

# Application Settings
# ------------------
# Defines the environment the application runs in
NODE_ENV=development
# Port number the server will listen on (if applicable)
PORT=3000

# Timeout Settings (in milliseconds)
# --------------------------------
API_TIMEOUT=30000
SCRAPE_TIMEOUT=60000

# Debug Settings
# ------------
# Set to 'true' to enable detailed logging
DEBUG=false

